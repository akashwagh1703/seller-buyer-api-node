<IfModule mod_rewrite.c>
    RewriteEngine On
    
    # Block TRACE requests
    RewriteCond %{REQUEST_METHOD} ^TRACE
    RewriteRule ^.*$ - [F,L]

    # Your existing rules
    RewriteCond $1 !^(index\.php|resources|robots\.txt)
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ index.php/$1 [L,QSA]
</IfModule>

<IfModule mod_headers.c>
    SetEnvIf Origin "https://api.nerace.in$" CORS_ALLOWED_ORIGIN
    SetEnvIf Origin "https://seller.nerace.in$" CORS_ALLOWED_ORIGIN
    SetEnvIf Origin "https://buyer.nerace.in$" CORS_ALLOWED_ORIGIN
    SetEnvIf Origin "https://portal.nerace.in$" CORS_ALLOWED_ORIGIN
    SetEnvIf Origin "https://support.nerace.in$" CORS_ALLOWED_ORIGIN
    SetEnvIf Origin "https://uatbuyer.nerace.in$" CORS_ALLOWED_ORIGIN
    SetEnvIf Origin "https://uatseller.nerace.in$" CORS_ALLOWED_ORIGIN
    SetEnvIf Origin "https://uatapi.nerace.in$" CORS_ALLOWED_ORIGIN
    SetEnvIf Origin "https://uatportal.nerace.in$" CORS_ALLOWED_ORIGIN
    SetEnvIf Origin "https://uatsupport.nerace.in$" CORS_ALLOWED_ORIGIN
    SetEnvIf Origin "https://uat.nerace.in$" CORS_ALLOWED_ORIGIN
    SetEnvIf User-Agent "Postman" CORS_ALLOWED_ORIGIN
    #SetEnvIf Content-Type "(multipart/form-data)" CORS_ALLOWED_ORIGIN

    Header always set Access-Control-Allow-Origin "*" env=CORS_ALLOWED_ORIGIN
    Header always set Access-Control-Allow-Methods "*"
    Header always set Access-Control-Allow-Headers "*"
    Header always set Access-Control-Allow-Credentials "true"
    Header always set Access-Control-Max-Age "86400"
</IfModule>

